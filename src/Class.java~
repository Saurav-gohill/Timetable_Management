public class Class {
    private Subject subject;
    private Faculty faculty;
    private Room room;
    private String time;

    public Class(Subject subject, Faculty faculty, Room room, String time) {
        this.subject = subject;
        this.faculty = faculty;
        this.room = room;
        this.time = time;
    }

    // Getters
    public Subject getSubject() {
        return subject;
    }

    public Faculty getFaculty() {
        return faculty;
    }

    public Room getRoom() {
        return room;
    }

    public String getTime() {
        return time;
    }
}
